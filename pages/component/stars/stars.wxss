/*page {
  position: absolute;
}*/

.star-image {
  position: absolute;
  top: 50rpx;
  width: 150rpx;
  height: 150rpx;
  src: "../../img/empty.png";
}

.item {
  position: absolute;
  top: 0rpx;
  width: 75rpx;
  height: 150rpx;
}

.stars-wrapper {
  width: 100%;
  height: 150rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  margin-top: 300rpx;
}

.star-full {
  color: #ffcb00;
  font-size: 80rpx;
}

.star-empty {
  color: #cbcbcb;
  font-size: 80rpx;
}
